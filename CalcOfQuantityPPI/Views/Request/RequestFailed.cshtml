@{
    ViewBag.Title = "Офромление заявки";
}

<div class="container"
     style="padding: 0 20px 20px 20px;
     background-color: rgb(255,255,255);
     box-shadow: rgba(0,0,0,0.5) 0 5px 20px;
     border-radius:5px;
     min-height:800px">
    <div class="row" style="background-color:rgb(0, 140, 208); margin-left:-20px; margin-right:-20px; padding:10px;">
        <div class="col-md-12">
            <h2 style="color:white;">Оформление заявки</h2>
        </div>
    </div>
    <br />
    <div class="row">
        <div class="col-md-12">
            <center>
                <h2 style="color:red;">Вы не указали численность сотрудников и/или количество СИЗ ни для одной профессии.</h2>
                @Html.ActionLink("Вернуться назад", "CreateRequest", "Request")
            </center>
        </div>
    </div>
</div>